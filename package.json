{
  "name": "react-app",
  "version": "1.0.0",
  "description": "react-app",
  "main": "webpack.config.js",
  "base": "/",
  "scripts": {
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "start": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "build:subfolder": "cross-env DEPLOYMENT_TARGET=subfolder webpack --config webpack.prod.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx}\" --fix"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)+(spec|test).js?(x)"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|webp|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/",
      "<rootDir>/dist/",
      "<rootDir>/public/"
    ],
    "testEnvironment": "jsdom"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-throw-expressions": "^7.25.8",
    "@babel/plugin-syntax-jsx": "^7.25.7",
    "@babel/plugin-transform-async-to-generator": "^7.25.7",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-logical-assignment-operators": "^7.25.9",
    "@babel/plugin-transform-modules-commonjs": "^7.25.7",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.26.6",
    "@babel/plugin-transform-numeric-separator": "^7.25.9",
    "@babel/plugin-transform-object-rest-spread": "^7.25.9",
    "@babel/plugin-transform-optional-catch-binding": "^7.25.9",
    "@babel/plugin-transform-optional-chaining": "^7.25.9",
    "@babel/plugin-transform-private-methods": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.25.7",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-react": "^7.25.7",
    "@babel/preset-typescript": "^7.25.7",
    "@base-ui-components/react": "^1.0.0-alpha.7",
    "@testing-library/jest-dom": "^6.6.3",
    "assert": "^2.1.0",
    "babel-loader": "^9.2.1",
    "constants-browserify": "^1.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.3.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "html-webpack-plugin": "^5.6.2",
    "https-browserify": "^1.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "os-browserify": "^0.3.0",
    "prettier": "^3.3.3",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.4",
    "vm-browserify": "^1.1.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "webpack-merge": "^6.0.1"
  },
  "keywords": [
    "World of Whispers; Dark Chaos.",
    "Fantasy RPG"
  ],
  "author": "michaeladennis@yahoo.com",
  "license": "SEE LICENSE IN License.md",
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@faker-js/faker": "^9.2.0",
    "@mui/icons-material": "^6.1.4",
    "@mui/lab": "^6.0.0-beta.12",
    "@mui/styles": "^6.1.4",
    "@mui/x-date-pickers": "^7.21.0",
    "@reduxjs/toolkit": "^2.3.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "draft-js": "^0.11.7",
    "draftjs-to-html": "^0.9.1",
    "ethers": "^6.13.4",
    "formik": "^2.4.6",
    "html-to-draftjs": "^1.5.0",
    "immutability-helper": "^3.1.1",
    "js-file-download": "^0.4.12",
    "notistack": "^3.0.1",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.3",
    "react-idle-timer": "^5.7.2",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.27.0",
    "react-slick": "^0.30.2",
    "recharts": "^2.13.0",
    "serve": "^14.2.4",
    "slick-carousel": "^1.8.1",
    "source-map-explorer": "^2.5.3",
    "three": "^0.179.1",
    "web-vitals": "^4.2.3",
    "yup": "^1.4.0"
  }
}
